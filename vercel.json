{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["sfo1"],
  "env": {
    "PAYLOAD_SECRET": "@payload-secret",
    "DATABASE_URI": "@database-uri",
    "NEXT_PUBLIC_SERVER_URL": "@next-public-server-url",
    "PAYZEN_SHOP_ID": "@payzen-shop-id",
    "PAYZEN_MODE": "@payzen-mode",
    "PAYZEN_PROD_KEY": "@payzen-prod-key",
    "PAYZEN_HMAC_PROD_KEY": "@payzen-hmac-prod-key",
    "PAYZEN_PUBLIC_PROD_KEY": "@payzen-public-prod-key",
    "BLOB_READ_WRITE_TOKEN": "@blob-read-write-token"
  }
}
